#!/usr/bin/env bash

#Linux/Cygwin Colors for ls
export LS_COLORS="di=1;32;40:ln=36;40:so=34;40:pi=34;40:ex=1;35;40:bd=34;40:cd=34;40:su=1;35;40:sg=1;35;40:tw=32;40:ow=32;40:"

# Aliases specific to Linux
alias ls="ls --color -F"
[ `which ack-grep 2> /dev/null` ] && alias ack="ack-grep"

[ -r /etc/bash_completion ] && . /etc/bash_completion
[ -r /usr/share/doc/tmux/examples/bash_completion_tmux.sh ] && . /usr/share/doc/tmux/examples/bash_completion_tmux.sh
